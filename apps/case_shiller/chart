            labels: { style: { color: '#FFFFFF' } }
        },
        yAxis: {
            title: { text: 'Returns', align: 'high', style: { color: '#FFFFFF' } },
            labels: {
                overflow: 'justify',
                style: { color: '#FFFFFF' },
                format: '{value}%'
            }
        },
