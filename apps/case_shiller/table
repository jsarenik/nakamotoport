<script>
    $(document).ready(function () {

        // Enable Tooltips
        const tooltipTriggerList = document.querySelectorAll('[data-bs-toggle="tooltip"]')
        const tooltipList = [...tooltipTriggerList].map(tooltipTriggerEl => new bootstrap.Tooltip(tooltipTriggerEl))

        $('#indexTable').DataTable(
            {
                "autoWidth": true,
                "paging": false,
                "scrollX": false,

                "order": [[9, 'desc'], [0, 'asc']]
            }
        );

        var urlParams = new URLSearchParams(window.location.search);
        var no_btc = urlParams.get('no_btc');

        if (no_btc === 'true') {
            $('.include-btc').show();
            $('.exclude-btc').hide();
        } else if (no_btc === 'false') {
            $('.include-btc').hide();
            $('.exclude-btc').show();
        } else {
            // Handle the case where 'no_btc' parameter is not present
            // Here I've chosen to show both buttons but you can adjust as needed
            $('.include-btc').show();
            $('.exclude-btc').hide();
        }
    });
</script>

